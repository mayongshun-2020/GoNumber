*{
 margin:0px;
 padding:0px;
 box-sizing:border-box;
}
body{
 font-size:1em;
 font-family:Arial;
 
}
ul li{
 list-style:none;
}
a{
 display:block;
 text-decoration:none;
 transition:1.5s;
}
img{
 display:block;
}
.flex{
 display:flex;
}
header,section,footer{
 width:100%;
}
.content{
 width:1330px;
 margin:0 auto;
}
.fixed{
 position:fixed;
 top:0px;
 width:1330px;
 z-index:999;box-shadow:0 0 10px #666;
}

.top{
 height:80px;
 background:#000;
 position:relative;
 z-index:99;
 top:0;
}
.top img{
  width:230px;
  height:auto;
  position:absolute;
  top:50%;
  left:5%;  
}
.top ul{
 width:60%;
 float:right;
}
.top ul li{
 float:left;
}
.top ul li a{
 padding:0px 40px;
 line-height:80px;
 color:#fff;
 font-size:1.2em;
}
.top ul li a:hover{
 color:#ffcc00; 
}
.banner{
 width:100%;
 height:430px;
 position:relative;
}
.banner img{
 width:100%;
 height:100%;
}
.banner h2{
 position:absolute;
 top:30%; 
 right:200px;
 color:#ffcc00;
 font-size:3em;
 text-shadow:0 0 5px #000;
 animation:mymove 3s;
}
.banner a{ 
 width:200px;
 height:50px;
 background:#262626;
 color:#fff;
 margin:5px;
 border-radius:5px;
 text-align:center;
 line-height:50px;
 font-weight:bold;
 display:inline-block;
 box-shadow:0 0 5px blue;
}
.banner a:hover{
 background:blue;
}
.but{
 position:absolute;
 bottom:30px;
 left:100px;
 animation:mymove2 3s;
}
@keyframes mymove2{
 from{ bottom:0px;
 left:100px;}
 to{ bottom:30px;
 left:100px;}
}
@keyframes mymove
{
    from{right:40px;
	     color:#0033ff;
		 text-shadow:none;
	 }
    to{right:200px;
	   color:#ffcc00;
	   text-shadow:0 0 5px #000;
	}
}
.title{
 font-size:3em;
 text-align:center;
 line-height:60px; 
 margin:50px auto;
 width:80%;
}
.title p{
 width:30%;
 height:2px;
 background:blue;
 margin:10px auto;
}
.screenBox{
 width:760px;
 margin:10px auto;
 flex-wrap:wrap; 
}
.screenBox .sub{
 width:50%;
 height:250px;
 background:#6b66fb;
 color:#fff; 
 transform:rotateY(0deg);
 opacity:1; 
}

.sub img{
 width:100%;
 height:100%;
}
.sub h3{
 text-align:center; 
 border-bottom:2px solid #fff;
 width:80%;
 margin:30px auto 5px;
 line-height:40px;
}
.sub p{
 width:80%;
 margin:5px auto;
 line-height:30px;
 text-align:center;
}
.sub a{
 width:150px;
 height:30px;
 background:#ffcc00;
 text-align:center;
 line-height:30px;
 color:blue;
 margin:15px auto;
}
.sub a:hover{
 background:blue;
 color:#ffcc00;
 box-shadow:0 0 5px #fff;
}
.contactbg{
 height:470px;
 background:url(../images/timga.jpg);
 background-size:100% 100%;
 position:relative;
 width:100%;
 z-index: 1;
 color:#fff;
 text-align:center;
 box-sizing:border-box;
 padding-top:80px;
}
.contactbg::before{
    content: "";
    position: absolute;
    left: 0;
    top: 0;
    height:100%;
    width: 100%;
    background:#000;
    opacity: .9;
    z-index: -1;
}
.contactbg h3{
 font-size:3em;
}
.contactbg p{
 font-size:1.3em;
 margin:20px auto;
}
input[type="text"]{
 width:320px;
 height:40px;
 margin:3px;
 text-indent:10px;
}
.contactbg form{
 margin:25px auto;
 display:block;
}
input[type="submit"]{
 width:185px;
 height:40px;
 border:none;
 background:#6b66fb;
 font-size:1.3em;
 cursor:pointer;
}
input[type="submit"]:hover{
 background:#ffcc00;
 box-shadow:0 0 5px #ffcc00;
}
.list{
 position:relative;
 perspective:1000px;
 width:23%;
 height:225px; 
}
.talented{
 justify-content:space-between;
}
.list img{
 width:100%;
 height:100%;
 border:2px solid #262626;
 box-shadow:0 0 10px #262626;
 transform:rotateY(0deg);transition:1.5s;
}
.list:hover img{
 transform:rotateY(90deg);
}
.list .traninfo{
 width:100%;
 height:100%;
 background:rgba(0,0,0,0.9);
 position:absolute;
 top:0;left:0;
 text-align:center;
 color:#fff;
 transition:1.5s ease;
 transform:rotateY(-90deg);
}
.list:hover .traninfo{
 transform:rotateY(0deg);
}
.what{
 width:1115px;
 height:400px;
 margin:30px auto;
 overflow:hidden;
 box-shadow:0 0 10px #000; 
}
.what ul li{
 width:45px;
 height:400px;
 float:left;
 position:relative;
 transition:1.5s;
}
.what ul li.on{
 width:980px;
}
.what ul li.on a{
 background:#ffcc00;
 color:blue;
}
.what img{
 width:100%;
 height:100%;
}
.what ul li a{
   width:45px;height:400px;
	padding:100px 10px 0;
	box-sizing:border-box;
	background:#262626;
	color:#fff;
	text-decoration:none;
    border-right:1px solid #fff;
	position:absolute;
	top:0px;left:0px;
	z-index:3;
}
footer .content{
 height:240px;
 background:#000;
 margin-top:50px;
 color:#fff;
 padding:50px 20px;
 
}
footer img.logo{
 width:230px;
 height:70px;
}
.foot1{
 justify-content:space-between;
 font-size:1.3em;line-height:70px;
}
img.seo{
 margin:20px auto;
}
.gohome{
 width:60px;
 height:60px;
 background:#ffcc00;
 border-radius:5px;
 border:1px solid #fff;
 color:blue;
 box-shadow:0 0 10px #000;
 cursor:pointer;
 position:fixed;
 bottom:200px;
 right:20px;
 text-align:center;
 line-height:60px;
 font-size:1.5em;
 transition:1.5s;
 z-index:999;
}
.gohome:hover{
  transform:rotate(360deg);
  box-shadow:0 0 20px #000;
  background:blue;
  color:#fff;
}
.menu{
 display:none;
}
.menulist{
 display:none;
}
@media screen and (max-width:768px)
{ 
 .content{
  width:768px;
  transition:1.5s;
 }
 .fixed{
 position:fixed;
 top:0px;
 width:760px;
 z-index:999;box-shadow:0 0 10px #666;
}
.top img{
 width:180px;
 height:auto;
 top:63%;
}
.top ul li a{
 padding:0px 20px;
 font-size:1em;
}
.top ul{
 width:70%;
}
.banner{
 height:400px;
}
.title{
 font-size:2em;
}
.screenBox {
    width:700px;
    margin: 10px auto;
    flex-wrap: wrap;
}
.screenBox .sub{
 height:230px;
}
.sub h3 {  
    border-bottom: 2px solid #fff;
    width:90%;
    margin:15px auto 5px;
    line-height:20px;
}
.contactbg {
    height:420px;   
    padding-top:50px;
}
input[type="text"] {
    width:180px;
    height:35px;   
}
input[type="submit"] {
    width:150px;
    height:35px;    
    font-size: 1.3em;
   
}
.talented {
   flex-wrap:wrap;
}
.list {
    position: relative;
    perspective: 1000px;
    width:48%;
    height: 225px;
	margin:20px auto;
}
.what {
    width:760px;
    height:350px;
    margin:30px auto;
    overflow: hidden;
    box-shadow: 0 0 10px #000;
}
.what ul li.on{
 width:600px;
}
}
@media screen and (max-width:480px)
{ 
 .content{
  width:480px;
  transition:1.5s;
 }
 .fixed{ 
 width:480px; 
}
.top ul{
 display:none;
}
.menu{ 
 width:150px;
 height:30px; 
 text-align:center;
 line-height:30px;
 position:absolute;
 left:50%;
 top:30px;
 margin-left:-75px;
 border-radius:5px;
 color:#ffcc00;
 border:1px solid #ffcc00;
 box-shadow:0 0 10px #ffcc00;
 cursor:pointer;
 display:block;
}
.menulist{
 position:absolute;
 top:80px;
 left:0px;
 width:480px;
 background:rgba(0,0,0,0.7);
 z-index:10;
 box-shadow:0 2px 2px #fff; 
}
img.logo{
 z-index:9999;
}
.menulist p{
 line-height:50px;
 text-align:center;
 border-bottom:1px dashed #fff;
 
}
.menulist p a{
 color:#fff;
}
.banner a {
    width:100px;
    height:30px;
	line-height:30px;
}
.title{
 font-size:1.5em;
 width:95%;
}
.screenBox .sub {
    width:90%;
    height:280px;
    background: #6b66fb;
    color: #fff;
    transform: rotateY(0deg);
    opacity: 1;
	margin:2px auto;
}
.screenBox {
    width:480px; 
   
}
.sub h3 {
    border-bottom: 2px solid #fff;
    width:90%;
    margin:35px auto;
    line-height: 20px;
}
.contactbg {
    height:380px;
    padding-top:40px;
}
.contactbg h3{
 font-size:2em;
}
.contactbg p {
    font-size: 1em;
    margin: 20px auto;
}
.contactbg form {
    display:flex;
	flex-direction:column;
}
input[type="text"] {
    width:450px;
    height:40px;
	margin:5px auto;
}
input[type="submit"] {
    width:450px;
    height: 40px;
    border: none;
    background: #6b66fb;
    font-size: 1.3em;
    cursor: pointer;
	margin:5px auto;
}
.list {    
    width:90%;
    height:250px;
    margin: 20px auto;
}
.what{
 width:475px;
 height:300px;
}
.what ul li.on{
 width:380px;
}
.what ul li {
    width:30px;
    height:300px;
    float:left;
    position: relative;
    transition: 1.5s;
}
.what ul li a {
    width:30px;
    height:300px;
    padding:100px 1px 0;
    box-sizing: border-box;    
    border-right: 1px solid #fff;
    font-size:0.8em;
}
.foot1 img.logo{
 width:180px;
 height:50px;
}
.foot1{
 justify-content:space-between;
 font-size:1em;
}
}